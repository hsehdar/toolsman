Tor;cat /opt/tor/Browser/tbb_version.json | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/tor;curl -Ls https://dist.torproject.org/torbrowser/ | grep folder | grep -oE '([0-9]{1,}\.)+[0-9]{1,}' | sort -ru | head -n1;https://dist.torproject.org/torbrowser/${AvailableVersion}/tor-browser-${OS}64-${AvailableVersion}_en-US.tar.xz;
Android Platform Tools;adb --version | grep Version | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/google_dev_tools/android/sdk;curl -Ls https://android.googlesource.com/platform/prebuilts/fullsdk-linux/platform-tools/+/refs/heads/master | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://dl.google.com/android/repository/platform-tools-latest-${OS}.zip;
CertBot;cat /opt/eff/version;/opt/eff/bin;curl -Ls https://api.github.com/repos/certbot/certbot/releases/latest | grep 'tag_name' | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://dl.eff.org/certbot-auto;
Google Chrome;/opt/google/chrome/chrome --version | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/google/chrome;curl -Ls https://dl.google.com/linux/chrome/deb/dists/stable/main/binary-amd64/Packages.gz | gunzip -c | grep -A1 'Package: google-chrome-stable$' | grep -oP '(?<=Version: ).*';https://dl.google.com/linux/chrome/deb/pool/main/g/google-chrome-stable/google-chrome-stable_${AvailableVersion}_${ARCH}.deb;
Helm;helm version --template='{{ .Version }}' | tr -d [A-Z][a-z];/opt/helm/bin;curl -Ls https://api.github.com/repos/helm/helm/releases | grep 'tag_name' | grep -oE '([0-9]{1,}\.)+[0-9]{1,}' | sort --version-sort -ur | head -n1;https://get.helm.sh/helm-v${AvailableVersion}-${OS}-${ARCH}.tar.gz;
Minikube;minikube update-check | grep Current | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/google_dev_tools/minikube/bin;minikube update-check | grep Latest | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://storage.googleapis.com/minikube/releases/latest/minikube-${OS}-${ARCH};
Kubectl;kubectl version --client --short=true | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/google_dev_tools/kubernetes/bin;curl -sL https://storage.googleapis.com/kubernetes-release/release/stable.txt | tr -d [A-Z][a-z];https://storage.googleapis.com/kubernetes-release/release/v${AvailableVersion}/bin/${OS}/${ARCH}/kubectl;
RClone;rclone --version | grep rclone | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/rclone/bin;curl -Ls https://downloads.rclone.org/version.txt | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://downloads.rclone.org/rclone-current-${OS}-${ARCH}.zip;
Repo;repo version | grep launch | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/google_dev_tools/android/build/bin;curl -Ls https://gerrit.googlesource.com/git-repo/+refs/?format=json | grep tags | sort -ur | head -n1 | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://storage.googleapis.com/git-repo-downloads/repo;
Vagrant;vagrant version | grep Installed | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/hashicorp/vagrant/bin;vagrant version | grep Latest | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://releases.hashicorp.com/vagrant/${AvailableVersion}/vagrant_${AvailableVersion}_${OS}_${ARCH}.zip;
Ventoy;cat /opt/ventoy/ventoy/version;/opt/ventoy;curl -Ls https://api.github.com/repos/ventoy/Ventoy/releases/latest | grep 'tag_name' | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://github.com/ventoy/Ventoy/releases/download/v${AvailableVersion}/ventoy-${AvailableVersion}-${OS}.tar.gz;
Go Language;cat /opt/google_dev_tools/go/VERSION | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';/opt/google_dev_tools/go;curl -Ls https://go.googlesource.com/go/+refs/?format=json | grep -oE 'tags\/go([0-9]{1,}\.)+[0-9]{1,}\"' | sort -ru --version-sort | head -n1 | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://golang.org/dl/go${AvailableVersion}.${OS}-${ARCH}.tar.gz;
Flutter;cat /opt/google_dev_tools/flutter/version;/opt/google_dev_tools/flutter;curl -Ls https://github.com/flutter/flutter/releases | grep "\/releases\/tag" | grep -oE '([0-9]{1,}\.)+[0-9]{1,}\"' | sort -ur --version-sort | head -n1 | grep -oE '([0-9]{1,}\.)+[0-9]{1,}';https://storage.googleapis.com/flutter_infra/releases/stable/linux/flutter_${OS}_${AvailableVersion}-stable.tar.xz;
